    <validationRules>
        <fullName>Client_Account_Edit_Permissions</fullName>
        <active>true</active>
        <description>Only Internal BFG users with access to the Client Account field are able to edit after creation.</description>
        <errorConditionFormula>IF(
  AND(
   ISCHANGED( Client_Account__c), 
   CONTAINS($Profile.Name, &quot;External BFG User&quot;)
  ), TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>Client_Account__c</errorDisplayField>
        <errorMessage>Only Internal BFG users can edit Client Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Billing_Method_to_Create_Record</fullName>
        <active>true</active>
        <description>The Billing Method field on SSS should be populated for a Production record to be created.</description>
        <errorConditionFormula>ISPICKVAL(Shared_Solar_System__r.Billing_Method__c, &quot;&quot;)</errorConditionFormula>
