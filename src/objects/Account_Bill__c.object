        <externalId>false</externalId>
        <formula>&quot;&amp;solmgr=1&quot; + 
&quot;&amp;id=&quot; + Id + 
&quot;&amp;queryid=[Sbs]a27j0000000bSg9&quot; + 
&quot;&amp;queryid=[Lbs]a270a000000NFEW&quot; +
&quot;&amp;templateid=&quot; + Bill_Template__r.Id + if(ISBLANK(Additional_Bill_Insert__r.Id),&apos;&apos;,&apos;,&apos; + Additional_Bill_Insert__r.Id) +
&quot;&amp;defaultpdf=1&quot; + 
&quot;&amp;ofn=&quot; + SUBSTITUTE(Name,&quot;&amp;&quot;,&quot;{~}&quot;) + &quot; Community Solar Bill&quot; + 
&quot;&amp;sc0=1&quot; + 
&quot;&amp;sc1=SalesforceFile&quot; + 
&quot;&amp;ac0=1&quot; + 
&quot;&amp;ac1=Bill Generated [&quot; + TEXT(Parent_Account__r.BillingStateCode) + &quot;]&quot; +
&quot;&amp;UF0=1&quot; + 
&quot;&amp;MFTS0=Ready_for_Email_Send__c&quot; + 
&quot;&amp;MFTSValue0=&quot; + IF(Parent_Account__r.Do_not_Send_Bills__c=true,&quot;false&quot;,&quot;true&quot;) + 
&quot;&amp;MFTS1=Ready_for_Paper_Bill_Queue__c&quot; + 
&quot;&amp;MFTSValue1=&quot; + IF(OR(ISPICKVAL(Parent_Account__r.Bill_Delivery_Preference__c,&quot;Email Only&quot;),Parent_Account__r.Do_not_Send_Bills__c=true),&quot;false&quot;,&quot;true&quot;) + 
&quot;&amp;MFTS2=Bill_PDF_Page_Count__c&quot; + 
&quot;&amp;MFTSValue2=PDF_PAGE_COUNT&quot; + 
&quot;&amp;qmode=SalesforceFile&quot;</formula>
        <label>Send Bill Conga Calc</label>
