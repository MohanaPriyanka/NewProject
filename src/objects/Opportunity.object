    <fields>
        <fullName>All_Customers_Signature_Status__c</fullName>
        <description>This field tracks whether all of the Customers on Residential Loan and Community Solar opportunities have signed, taking into account Joint and Individual applications.</description>
        <externalId>false</externalId>
        <formula>CASE(
Product_Line__c,

&quot;Community Solar&quot;,

IF(Contract_Status__c =&quot;Completed&quot;,&quot;Completed&quot;,&quot;Incomplete&quot;),

&quot;Residential Loan&quot;,

CASE(Application_Type__c,
&quot;Joint&quot;,

IF(
AND(Contract_Status__c =&quot;Completed&quot;, Co_Applicant_Signature_Status__c=&quot;Completed&quot;),
&quot;Completed&quot;,&quot;Incomplete&quot;
),

&quot;Individual&quot;,
IF(Contract_Status__c =&quot;Completed&quot;,&quot;Completed&quot;,&quot;Incomplete&quot;),

&quot;&quot;),

&quot;&quot;
)</formula>
        <inlineHelpText>This field tracks whether all of the Customers on Residential Loan and Community Solar opportunities have signed, taking into account Joint and Individual applications.</inlineHelpText>
        <label>All Customers Signature Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>All_QC_Boxes_True__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
 Product_Line__c=&quot;Community Solar&quot;,
and(
 Contract_Fully_Populated__c =true,  
 Confirmed_Subscription_Size_Correct__c =true,  
 Service_Address_and_Name_on_Account_Chk__c =true,
 Load_Zone_Quality_Check__c =true,
 Current_Utility_Bill__c = true,
 Green_Button_Data_Received__c = true,
 QC_Personal_Credit_Report__c = true
),
IF(
 Product_Line__c=&quot;Residential Loan&quot;,
and( Commencement_Date_QC__c=true,
 Loan_Amount_Verified__c =true,
 Customer_Address_Verified__c =true,
 System_Information_Verified__c,
 Customer_Signature_Verified__c ),false)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>All QC Boxes True?</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
