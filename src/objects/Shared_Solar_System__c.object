    <validationRules>
        <fullName>Client_Account_Validation</fullName>
        <active>true</active>
        <description>Checks to make sure that the Client Account on the SSS is the same as the Client Account on the Entity.</description>
        <errorConditionFormula>(
Client_Account__c != BWC_Project_Entity_Manual__r.Client_Account__c
&amp;&amp;
 ISBLANK(BWC_Project_Entity_Manual__r.Client_Account__c
)=false

)</errorConditionFormula>
        <errorMessage>The Client Account on the Shared Solar System and the Client Account on the Entity are not the same. If the Client Account on the Entity is not filled in, make sure that it is.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Acquisition_Credit_Score_Requir_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Stage__c),
ISPICKVAL(Stage__c,&quot;Acquisition&quot;),
OR (
Credit_Score_Requirement__c = 0,
ISBLANK(Credit_Score_Requirement__c)
)
)</errorConditionFormula>
        <errorMessage>Cannot change status to acquisition without populating the Credit Score Requirement field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Acquisition_Estimated_COD_PTO_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Stage__c),
ISPICKVAL(Stage__c,&quot;Acquisition&quot;),
OR (
ISBLANK(Estimated_COD_Date_Manual__c),
ISBLANK(Estimated_PTO_Date_MANUAL__c)
)
)</errorConditionFormula>
        <errorMessage>Cannot change status to acquisition without populating the Estimated COD and PTO dates</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Acquisition_Product_UNMC_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Stage__c),
ISPICKVAL(Stage__c,&quot;Acquisition&quot;),
OR(
ISBLANK(Product__c),
ISBLANK(Utility_NMC_Tariff__c)
)
)</errorConditionFormula>
        <errorMessage>Cannot change status to acquisition without populating the Product and Utility NMC Tariff (credit value)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Acquisition_System_Size_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Stage__c),
ISPICKVAL(Stage__c,&quot;Acquisition&quot;),
OR (
Total_System_Size_kWh_DC__c = 0,
ISBLANK(Total_System_Size_kWh_DC__c)
)
)</errorConditionFormula>
        <errorMessage>Cannot change status to acquisition without populating the System Size KW DC field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Acquisition_Utility_LZ_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Stage__c),
ISPICKVAL(Stage__c,&quot;Acquisition&quot;),
OR(
ISBLANK(Utility__c),
ISBLANK(Service_Territories__c)
)
)</errorConditionFormula>
        <errorMessage>Cannot change status to acquisition without populating the utility and load zone</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Acquisition_Yield_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Stage__c),
ISPICKVAL(Stage__c,&quot;Acquisition&quot;),
OR (
Expected_Yield_kWh_kW__c = 0,
ISBLANK(Expected_Yield_kWh_kW__c)
)
)</errorConditionFormula>
        <errorMessage>Cannot change status to acquisition without populating the Expected Yield field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Client_Account_Validation</fullName>
        <active>true</active>
        <description>Checks to make sure that the Client Account on the SSS is the same as the Client Account on the Entity.</description>
        <errorConditionFormula>(
Client_Account__c != BWC_Project_Entity_Manual__r.Client_Account__c
&amp;&amp;
 ISBLANK(BWC_Project_Entity_Manual__r.Client_Account__c
)=false

)</errorConditionFormula>
        <errorMessage>The Client Account on the Shared Solar System and the Client Account on the Entity are not the same. If the Client Account on the Entity is not filled in, make sure that it is.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Management_Fully_Subscribed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Stage__c),
ISPICKVAL(Stage__c,&quot;Management&quot;),
OR(
ISBLANK(Total_System_Size_kWh_DC__c),
Total_System_Size_kWh_DC__c = 0, 
Capacity_Available_to_be_Reserved__c &gt; 10
)
)</errorConditionFormula>
        <errorMessage>Cannot change status to management without fully subscribing the project</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Management_Open_Is_False</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Stage__c),
ISPICKVAL(Stage__c,&quot;Management&quot;),
Open__c
)</errorConditionFormula>
        <errorMessage>Cannot change status to management without first closing the SSS for sales</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NMC_Tariff_Not_Retired</fullName>
        <active>true</active>
        <description>A Shared Solar System shouldn&apos;t have a retired tariff.</description>
        <errorConditionFormula>AND(

ISPICKVAL(Utility_NMC_Tariff__r.Status__c,&quot;Retired&quot;) ,

ISCHANGED(Utility_NMC_Tariff__c)

)</errorConditionFormula>
        <errorMessage>You cannot use an NMC Tariff with Status=Retired. Use an active tariff, or change the tariff status to active if it is active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Onboarding_Client_Account_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Stage__c),
ISPICKVAL(Stage__c,&quot;Onboarding&quot;),
ISBLANK(Client_Account__c)
)</errorConditionFormula>
        <errorMessage>Cannot change status to onboarding without linking an SSS to a client account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Open_SSS</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Open__c),
Open__c,
OR(
ISBLANK(Capacity_Available_to_be_Reserved__c),
Capacity_Available_to_be_Reserved__c = 0 ,
ISBLANK(Total_System_Size_kWh_DC__c),
Total_System_Size_kWh_DC__c = 0,
ISBLANK(Expected_Yield_kWh_kW__c),
Expected_Yield_kWh_kW__c = 0,
ISBLANK(Sales_Partners__c),
ISPICKVAL(Assignment_Order__c,&quot;&quot;),
ISBLANK(Product__c),
ISBLANK(Utility_NMC_Tariff__c),
ISBLANK(Utility__c),
ISBLANK(Service_Territories__c),
ISBLANK(Service_Territories__c),
ISBLANK(Maximum_Subscription_Cost__c ),
Maximum_Subscription_Cost__c = 0,
ISBLANK(Credit_Score_Requirement__c),
Credit_Score_Requirement__c = 0
)
)</errorConditionFormula>
        <errorMessage>Can&apos;t set a SSS as Open without:
1.) System Size and Yield
2.) Capacity Available to Be Reserved &gt; 0 
3.) Product and Utility NMC Tariff
4.) Utility and Service Territories
5) Maximum Subscription Cost &gt; 0
6.) Credit Score Requirement</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PM_Team_Locks</fullName>
        <active>true</active>
        <description>Prevents users who do not have the &quot;Allow Edit of SSS Address&quot; permission from changing the SSS address.</description>
        <errorConditionFormula>NOT($Permission.Allow_Edit_of_SSS_Address) &amp;&amp;
    OR(
    ISCHANGED(Address__c),
    ISCHANGED(City__c),
    ISCHANGED(State__c),
    ISCHANGED(Zip_Code__c),
    ISCHANGED(Meter__c),
    ISCHANGED(Host_Utility_Account_Number__c),
    ISCHANGED(App_ID_WO_Manual__c)
    )</errorConditionFormula>
        <errorMessage>Due to their importance, certain fields on the Shared Solar System page are restricted so that only certain people can edit them. Please contact your System Administrator to edit.</errorMessage>
    </validationRules>
