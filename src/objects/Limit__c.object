    <validationRules>
        <fullName>LMT001_TemporaryThresholdReqExpiration</fullName>
        <active>true</active>
        <description>When a temporary threshold is entered, an expiration date time is needed</description>
        <errorConditionFormula>NOT(ISBLANK(TemporaryAlertThreshold__c)) &amp;&amp; ISBLANK( TemporaryAlertThresholdExpiration__c)</errorConditionFormula>
        <errorDisplayField>TemporaryAlertThresholdExpiration__c</errorDisplayField>
        <errorMessage>When a Temporary Alert Threshold is entered, the Temporary Alert Threshold Expiration is needed. [LMT-001]</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LMT002_TemporaryThresholdReqExpiration</fullName>
        <active>true</active>
        <description>When a an expiration date time is entered, temporary threshold is needed</description>
        <errorConditionFormula>ISBLANK(TemporaryAlertThreshold__c) &amp;&amp; NOT(ISBLANK( TemporaryAlertThresholdExpiration__c))</errorConditionFormula>
        <errorDisplayField>TemporaryAlertThreshold__c</errorDisplayField>
        <errorMessage>When a Temporary Alert Threshold Expiration is entered, the Temporary Alert Threshold is needed. [LMT-002]</errorMessage>
