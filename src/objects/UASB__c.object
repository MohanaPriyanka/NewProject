    <fields>
        <fullName>Allocated_Late_Fees__c</fullName>
        <externalId>false</externalId>
        <formula>(Discounted_Bill__c/System_Bill__r.Discounted_Bill__c ) * (System_Bill__r.Late_Fee_Payments__c)</formula>
