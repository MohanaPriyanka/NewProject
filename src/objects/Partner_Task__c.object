    &quot;Provide Tax Return (Previous Year)&quot;, &quot;Upload&quot;,
    &quot;Provide Tax Return (Two Years Previous)&quot;, &quot;Upload&quot;,
    &quot;Provide Technical Confirmation Documentation&quot;, &quot;Upload&quot;,
    &quot;Provide all System Information&quot;, &quot;Input&quot;,
    &quot;Under BlueWave Review&quot;, &quot;&quot;, 
    &quot;Obtain Contract Signature&quot;, &quot;Upload&quot;,
    &quot;Provide Sales Agreement&quot;, IF(ISBLANK(Loan__r.Opportunity__c),&quot;ERROR, call BW&quot;, &quot;Upload&quot;),
    &quot;BlueWave Counter Signature&quot;, &quot;&quot;,
    &quot;Building Permit&quot;, &quot;Submit&quot;,
    &quot;Mechanical Installation&quot;, &quot;Upload&quot;,
    &quot;Interconnection&quot;, IF( Loan__r.State__c = &quot;MA&quot;, &quot;Input&quot;, &quot;Upload&quot;),
    &quot;Report Interconnection to MCEC&quot;, IF( Loan__r.State__c = &quot;MA&quot;, &quot;Input&quot;, &quot;Upload&quot;),
    &quot;Complete&quot;, &quot;&quot;,
    &quot;&quot;)</formula>
