    <validationRules>
        <fullName>Interconnected_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL( Status__c ,&apos;Pending&apos;)|| 
ISPICKVAL( Status__c ,&apos;Incomplete&apos;)|| 
ISPICKVAL( Status__c, &apos;&apos;)) &amp;&amp; 
ISPICKVAL(PRIORVALUE(Status__c),&quot;Complete&quot;) &amp;&amp; 
Task_Type__c = &apos;Interconnection&apos; &amp;&amp; 
Opportunity__r.Interconnected__c = true</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>This task status cannot be changed after the system is interconnected.  Please uncheck the &apos;Interconnected&apos; checkbox on the Residential Equipment to set this status to &apos;Pending&apos;.</errorMessage>
    </validationRules>
