    <fields>
        <fullName>Stage_Duration__c</fullName>
        <description># of Business Days spent in &quot;New Stage&quot;</description>
        <externalId>false</externalId>
        <formula>(ROUND( 9 * (
   ( 5 * FLOOR( ( DATEVALUE(  Closed_On__c  ) - DATE( 1900, 1, 8) ) / 7) +
    MIN(5, 
     MOD( DATEVALUE(  Closed_On__c  ) - DATE( 1900, 1, 8), 7) +
     MIN( 1, 24 / 8 * ( MOD( Closed_On__c - DATETIMEVALUE( &apos;1900-01-08 13:00:00&apos; ), 1 ) ) )
    ) 
   )
 -
   ( 5 * FLOOR( ( DATEVALUE(  CreatedDate  ) - DATE( 1900, 1, 8) ) / 7) +
     MIN( 5,
      MOD( DATEVALUE( CreatedDate ) - DATE( 1996, 1, 1), 7 ) +
      MIN( 1, 24 / 8 * ( MOD( CreatedDate - DATETIMEVALUE( &apos;1900-01-08 13:00:00&apos; ), 1) ) )
    )
   ) 
  ), 
0 ))/9</formula>
        <inlineHelpText># of Business Days spent in &quot;New Stage&quot;</inlineHelpText>
        <label>Stage Duration</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
