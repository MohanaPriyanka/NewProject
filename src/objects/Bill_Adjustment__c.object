    <validationRules>
        <fullName>Adjustment_Type_Is_Required</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
ISPICKVAL(Adjustment_Type__c,&quot;&quot;),
ISPICKVAL(Approval_Status__c ,&quot;Approved&quot;)
)</errorConditionFormula>
        <errorMessage>Adjustment Type is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Modify_Amount_After_Applied</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(UASB__c)), 
ISCHANGED(Adjustment_Amount__c), 
UASB__r.Adjustment_Amount__c != Adjustment_Amount__c)</errorConditionFormula>
        <errorMessage>Cannot modify adjustment amount without first changing the UASB Adjustment Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Production_Type_Must_Have_NMC_Value</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISPICKVAL(Adjustment_Type__c, &apos;Production&apos;),  ISBLANK( Credits_Allocated__c) , FALSE)</errorConditionFormula>
        <errorDisplayField>Credits_Allocated__c</errorDisplayField>
        <errorMessage>Bill Adjustments from Production Errors must have Credits Allocated filled in</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
