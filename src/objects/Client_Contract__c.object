    <fields>
        <fullName>Effective_Date__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>At the beginning of this day, the system will automatically populate the Shared Solar System with the related Contract according to the CS Services Type to be used for Client Invoicing.</inlineHelpText>
        <label>Effective Date</label>
        <required>false</required>
