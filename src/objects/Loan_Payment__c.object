    <fields>
        <fullName>Actual_Interest_Due__c</fullName>
        <externalId>false</externalId>
        <formula>IF(OR(ISPICKVAL(Bill_Status__c, &quot;Current&quot;), ISPICKVAL(Bill_Status__c, &quot;Closed&quot;)),
   IF (Loan_Balance__c &lt;= 0, 
       0, 
       Loan_Balance__c * Interest_Rate__c / 12),
   null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Loan Balance * Monthly Rate from related Product on Loan</inlineHelpText>
        <label>Actual Interest Due</label>
        <precision>18</precision>
        <required>false</required>
        <scale>5</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
