        <fullName>Payment_Due_Date_formula__c</fullName>
        <externalId>false</externalId>
        <formula>if ( ISBLANK( Payment_Due_Date2__c ) ,
     DATE(YEAR(TODAY())+FLOOR((MONTH(TODAY())+Period__c)/12),
          CASE(MOD(MONTH(TODAY())+1+MOD(Period__c,12),12),0,12,
               MOD(MONTH(TODAY())+1+MOD(Period__c,12),12)),
