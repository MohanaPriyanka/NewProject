    <fields>
        <fullName>Late_Category__c</fullName>
        <externalId>false</externalId>
        <formula>IF(

Days_late__c  &lt;= 0, &quot;On Time&quot;,

IF(

Days_late__c &lt;= 30, &quot;0-30 days&quot;,

IF(

(Days_late__c &gt; 30 &amp;&amp; Days_late__c &lt;= 60), &quot;30-60 days&quot;,


IF(

(Days_late__c &gt; 60 &amp;&amp; Days_late__c &lt;= 90), &quot;60-90 days&quot;,

IF(

(Days_late__c &gt; 90 &amp;&amp; Days_late__c &lt;= 120), &quot;90-120 days&quot;,

&quot;over 120 days&quot;)))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Late Category</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
