IF(

Days_late__c &lt;= 30, &quot;0-30 days&quot;,

IF(

(Days_late__c &gt; 30 &amp;&amp; Days_late__c &lt;= 60), &quot;30-60 days&quot;,


IF(

(Days_late__c &gt; 60 &amp;&amp; Days_late__c &lt;= 90), &quot;60-90 days&quot;,

