        <label>Applied to Account?</label>
        <required>true</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>No</fullName>
                    <default>true</default>
                    <label>No</label>
                </value>
                <value>
                    <fullName>Yes</fullName>
                    <default>false</default>
                    <label>Yes</label>
                </value>
                <value>
                    <fullName>Not Applied in Salesforce</fullName>
                    <default>false</default>
                    <label>Not Applied in Salesforce</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Check_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISBLANK(Override_Check_Date__c), DATEVALUE( CreatedDate ),  Override_Check_Date__c )</formula>
