        <columns>Anchor__c</columns>
        <columns>NAME</columns>
        <columns>Account__c</columns>
        <columns>Amount__c</columns>
        <columns>Applied_to_Account__c</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
        <label>Paper Checks Name</label>
        <trackHistory>true</trackHistory>
        <type>Text</type>
    </nameField>
    <pluralLabel>Paper Checks</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Enforce_Unique_Names</fullName>
        <active>true</active>
        <errorConditionFormula>AND(IF(
VLOOKUP($ObjectType.Paper_Check__c.Fields.Name, $ObjectType.Paper_Check__c.Fields.Name,  Name )= Name,true,false), ISNEW()=true)</errorConditionFormula>
        <errorMessage>Checks must have a unique BlueWave Account number and Check number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Changes_After_Logging</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
$User.ProfileId!=&apos;00ej0000000tMiv&apos;,
TEXT(Applied_to_Account__c)=&apos;No&apos;,
OR(ISCHANGED(Amount__c)=true,
ISCHANGED(Account__c)=true,
ISCHANGED(Check_Number__c),
ISCHANGED(Applied_to_Account__c),
ISCHANGED(Name)
))</errorConditionFormula>
        <errorMessage>Paper Checks cannot be modified after they&apos;ve been applied to an Account.</errorMessage>
    </validationRules>
