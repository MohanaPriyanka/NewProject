{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)}

sforce.apex.execute(&quot;ReturnedTransactionHandler&quot;,&quot;unrollPaperCheck&quot;,{transactionId:&quot;{! Paper_Check__c.Id}&quot;}); 
location.reload(true);</url>
    </webLinks>
