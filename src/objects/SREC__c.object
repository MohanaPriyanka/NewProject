        <externalId>false</externalId>
        <formula>if(Static_SREC_Product__r.Fixed_Payment_per_kW_DC__c!=0,0, 
CASE( 
Static_SREC_Product__r.Fixed_Payment__c, 
0, (Payment_SREC__c -(Percentage_Based_Commission__c*(Clearing_Price__c))-Fixed_Rate_Commission_per_SREC__c), 
Static_SREC_Product__r.Fixed_Payment__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>This is the Payment/SREC minus the BlueWave Fee per SREC</inlineHelpText>
        <label>CalculatedÂ Price/SREC to Customer</label>
        <precision>18</precision>
