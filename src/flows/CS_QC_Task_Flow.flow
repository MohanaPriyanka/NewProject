    <recordUpdates>
        <description>If Flow launched from Lead, update the Lead Application Status field to &quot;Awaiting Information Requested&quot;</description>
        <name>Update_Lead_Application_Status</name>
        <label>Update Lead Application Status</label>
        <locationX>1440</locationX>
        <locationY>497</locationY>
        <filters>
            <field>Id</field>
