    <decisions>
        <description>Check for null</description>
        <name>Is_there_a_Sales_Rep</name>
        <label>Is there a Sales Rep</label>
        <locationX>445</locationX>
        <locationY>51</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Rep_not_null_0</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>sovOpportunity.BS_Sales_ID__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Contact_for_Partner</targetReference>
            </connector>
            <label>Rep != null</label>
        </rules>
        <rules>
            <name>Rep_is_Null_0</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>sovOpportunity.BS_Sales_ID__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_New_QC_Task</targetReference>
            </connector>
            <label>Rep = Null</label>
        </rules>
    </decisions>
    <decisions>
        <description>Check for null</description>
        <name>Is_there_Partner</name>
        <label>Is there  Partner</label>
        <locationX>284</locationX>
        <locationY>56</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Partner_not_null</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>sovOpportunity.Partner_tag_lookup__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>GetAccountforPartner</targetReference>
            </connector>
            <label>Partner != null</label>
        </rules>
        <rules>
            <name>Partner_is_Null</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>sovOpportunity.Partner_tag_lookup__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Is_there_a_Sales_Rep</targetReference>
            </connector>
            <label>Partner = Null</label>
        </rules>
    </decisions>
