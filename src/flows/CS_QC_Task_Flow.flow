<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <choices>
        <name>QCCaseType</name>
        <choiceText>Adjust subscription, &amp;gt;25kW</choiceText>
        <dataType>String</dataType>
    </choices>
    <decisions>
        <description>Check for null</description>
        <name>Is_there_a_Sales_Rep</name>
        <label>Is there a Sales Rep</label>
        <locationX>826</locationX>
        <locationY>346</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Rep_not_null_0</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varSalesPersonID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Contact_for_Partner</targetReference>
            </connector>
            <label>Rep != null</label>
        </rules>
        <rules>
            <name>Rep_is_Null_0</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varSalesPersonID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_New_QC_Task</targetReference>
            </connector>
            <label>Rep = Null</label>
        </rules>
    </decisions>
    <decisions>
        <description>If started from opportunity, Check for null partner value</description>
        <name>Is_there_Partner_Opp</name>
        <label>Is there Partner - Opp</label>
        <locationX>629</locationX>
        <locationY>353</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Partner_not_null</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varPartnerID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>GetAccountforPartner</targetReference>
            </connector>
            <label>Partner != null</label>
        </rules>
        <rules>
            <name>Partner_is_Null</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varPartnerID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Is_there_a_Sales_Rep</targetReference>
            </connector>
            <label>Partner = Null</label>
        </rules>
    </decisions>
    <decisions>
        <description>Determine whether flow has been launched from button on Opportunity or Lead object</description>
        <name>Launched_from_Opportunity_or_Lead</name>
        <label>Launched from Opportunity or Lead</label>
        <locationX>164</locationX>
        <locationY>393</locationY>
        <defaultConnector>
            <targetReference>Lookup_Opportunity</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Launched from Opportunity</defaultConnectorLabel>
        <rules>
            <name>Launched_from_Lead</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varOppID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Lookup_Lead</targetReference>
            </connector>
            <label>Launched from Lead</label>
        </rules>
    </decisions>
    <description>Start from Lead or Opportunity</description>
    <dynamicChoiceSets>
        <name>picklist_QCType</name>
        <dataType>Picklist</dataType>
        <displayField xsi:nil="true"/>
        <object xsi:nil="true"/>
        <picklistField>QC_Case_Subject__c</picklistField>
        <picklistObject>Case</picklistObject>
    </dynamicChoiceSets>
    <formulas>
        <name>formula_CaseSubject</name>
        <dataType>String</dataType>
        <expression>{!sovOpportunity.Name}&amp;&quot; - &quot;&amp;{!Case_Type}</expression>
    </formulas>
