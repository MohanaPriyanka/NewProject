        <expression>{!myVariable_current.Id}</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>&apos;Customer Cancellation - &apos; + [Opportunity].Name </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_5_myRule_1_A1_4139613926</name>
        <dataType>String</dataType>
        <expression>&apos;Customer Cancellation - &apos; + {!myVariable_current.Name}</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
[Opportunity].Product_Line__c = &quot;Community Solar&quot;,
ISCHANGED([Opportunity].StageName)=true,
ISPICKVAL(PRIORVALUE([Opportunity].StageName), &apos;Complete&apos;),
ISPICKVAL([Opportunity].StageName, &apos;Cancelled&apos;))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
