<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignments>
        <description>Counter for QC Tasks in loop</description>
        <name>assgQCTaskCounter</name>
        <label>assgQCTaskCounter</label>
        <locationX>714</locationX>
        <locationY>461</locationY>
        <assignmentItems>
            <assignToReference>varQCTaskCounter</assignToReference>
            <operator>Add</operator>
            <value>
                <numberValue>1.0</numberValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>loopQCTasks</targetReference>
        </connector>
    </assignments>
    <assignments>
        <description>Update Opp stage to QC In Process</description>
        <name>AssignOppStage</name>
        <label>AssignOppStage</label>
        <locationX>58</locationX>
        <locationY>602</locationY>
        <assignmentItems>
            <assignToReference>sovOpportunity.StageName</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>QC In Process</stringValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>updateOpportunityStage</targetReference>
        </connector>
    </assignments>
    <decisions>
        <description>Decision if there are still open QC tasks.</description>
        <name>decQCTaskNumber</name>
        <label>decQCTaskNumber</label>
        <locationX>54</locationX>
        <locationY>468</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>outcomeOpenQCTasks</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varQCTaskCounter</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <label>outcomeOpenQCTasks</label>
        </rules>
        <rules>
            <name>outcomeNoOpenTasks</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varQCTaskCounter</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>AssignOppStage</targetReference>
            </connector>
            <label>outcomeNoOpenTasks</label>
        </rules>
    </decisions>
    <description>Launched by a PB node when a CS QC Task closes. Checks to see if there are any open tasks -- If not, it sets the Opportunity stage to QC in Process.</description>
    <interviewLabel>Open QC Task Checker {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Open QC Task Checker</label>
    <loops>
        <description>Loops through open tasks to get count.</description>
        <name>loopQCTasks</name>
        <label>loopQCTasks</label>
        <locationX>566</locationX>
        <locationY>298</locationY>
        <assignNextValueToReference>loopvarQCTask</assignNextValueToReference>
        <collectionReference>covOpenQCTasks</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>assgQCTaskCounter</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>decQCTaskNumber</targetReference>
        </noMoreValuesConnector>
    </loops>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <description>Finds the sobject variable for the Opportunity. If it doesn&apos;t find a CS one, ends the flow.</description>
        <name>Find_CS_Opportunity</name>
        <label>Find CS Opportunity</label>
        <locationX>54</locationX>
        <locationY>152</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Lookup</targetReference>
        </connector>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>varOppID</elementReference>
            </value>
        </filters>
        <filters>
            <field>Product_Line__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Community Solar</stringValue>
            </value>
        </filters>
        <filters>
            <field>StageName</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Complete</stringValue>
            </value>
        </filters>
        <object>Opportunity</object>
        <outputReference>sovOpportunity</outputReference>
        <queriedFields>Id</queriedFields>
    </recordLookups>
    <recordLookups>
        <description>Looks up all of the QC tasks for the Opportunity passed through the Process Builders</description>
        <name>Lookup</name>
        <label>Lookup All QC Tasks</label>
        <locationX>56</locationX>
        <locationY>351</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>loopQCTasks</targetReference>
        </connector>
        <filters>
            <field>RecordTypeId</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>012j0000000ufwz</stringValue>
            </value>
        </filters>
        <filters>
            <field>Status</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Completed</stringValue>
            </value>
        </filters>
        <filters>
            <field>Status</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Verified Complete</stringValue>
            </value>
        </filters>
        <filters>
            <field>Status</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Redundant Task</stringValue>
            </value>
        </filters>
        <filters>
            <field>WhatId</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>varOppID</elementReference>
            </value>
        </filters>
        <object>Task</object>
        <outputReference>covOpenQCTasks</outputReference>
        <queriedFields>Id</queriedFields>
        <queriedFields>WhoId</queriedFields>
        <queriedFields>WhatId</queriedFields>
    </recordLookups>
    <recordUpdates>
        <description>If there are no open tasks, update the opportunity stage to</description>
        <name>updateOpportunityStage</name>
        <label>updateOpportunityStage</label>
        <locationX>289</locationX>
        <locationY>604</locationY>
        <inputReference>sovOpportunity</inputReference>
    </recordUpdates>
    <startElementReference>Find_CS_Opportunity</startElementReference>
    <status>Active</status>
    <variables>
        <description>Collection variable for all Open QC Tasks for an Opportunity.</description>
        <name>covOpenQCTasks</name>
        <dataType>SObject</dataType>
        <isCollection>true</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
        <objectType>Task</objectType>
    </variables>
    <variables>
        <name>loopvarQCTask</name>
        <dataType>SObject</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
        <objectType>Task</objectType>
    </variables>
    <variables>
        <name>sovOpportunity</name>
        <dataType>SObject</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
        <objectType>Opportunity</objectType>
    </variables>
    <variables>
        <name>varOppID</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>varQCTaskCounter</name>
        <dataType>Number</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
        <scale>0</scale>
    </variables>
</Flow>
