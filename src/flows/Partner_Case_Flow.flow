                <rightValue>
                    <stringValue>Load zone missing on utility bill</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>scr_UtilityBillUpload</targetReference>
            </connector>
            <label>Utility Bill Case</label>
        </rules>
        <rules>
            <name>dec_Underwriting_Case</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>get_Case.QC_Case_Subject__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Credit report did not match customer&apos;s name, attach credit check for right customer</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>get_Case.QC_Case_Subject__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Credit report is missing, Upload correct credit report</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>get_Case.QC_Case_Subject__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Custom&apos;s credit was denied , change stage to unqualified</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Underwriting_Type</targetReference>
            </connector>
            <label>Underwriting Case</label>
        </rules>
