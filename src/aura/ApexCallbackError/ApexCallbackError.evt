<aura:event type="APPLICATION" description="Error Handling Event">
    <aura:attribute name="className" type="String" access="global"/>
    <aura:attribute name="methodName" type="String" access="global"/>
    <aura:attribute name="errors" type="Object[]" access="global"/>
    <aura:attribute name="developerInfo" type="Object" access="global"/>
    <aura:attribute name="options" type="Object" access="global" description="See LightningErrorHandleController for options"/>
</aura:event>
