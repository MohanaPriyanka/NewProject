<aura:component implements="forceCommunity:availableForAllPageTypes" extends="c:BlueWaveParent" controller="NavHeaderController">
  <aura:attribute name="userName" type="String"/>
  <aura:attribute name="link" type="String" default="/slportal/s/slpdashboard"/>
  <aura:attribute name="showLogout" type="Boolean" default="true"/>
  <aura:handler name="init" value="{!this}" action="{!c.doInit}" /> 
  <div aura:id="header" class="header">
    <a href="{!v.link}">
      <div class="w3-container slds-align--absolute-center">
        <img src="{!$Resource.PerchLogoWhite}" class="logo fadeIn" />
      </div>
    </a>
  </div>
</aura:component>