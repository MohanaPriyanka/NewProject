<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Management_Open_Is_False</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISCHANGED(Stage__c),
ISPICKVAL(Stage__c,&quot;Management&quot;),
Open__c
)</errorConditionFormula>
    <errorMessage>Cannot change status to management without first closing the SSS for sales</errorMessage>
</ValidationRule>
