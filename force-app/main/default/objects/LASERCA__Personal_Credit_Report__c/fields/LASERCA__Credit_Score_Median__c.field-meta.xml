<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LASERCA__Credit_Score_Median__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>If (
	ISBLANK(LASERCA__Credit_Score_Equifax__c) || 
	ISBLANK(LASERCA__Credit_Score_Experian__c) || 
	ISBLANK(LASERCA__Credit_Score_TransUnion__c),
			(
					MIN(VALUE(BLANKVALUE(LASERCA__Credit_Score_Equifax__c, &apos;10000&apos;)), 
					VALUE(BLANKVALUE(LASERCA__Credit_Score_Experian__c, &apos;10000&apos;)), 
					VALUE(BLANKVALUE(LASERCA__Credit_Score_TransUnion__c, &apos;10000&apos;)))
			),

	MAX(MIN(VALUE(LASERCA__Credit_Score_Experian__c),VALUE(LASERCA__Credit_Score_TransUnion__c)), MIN(MAX(VALUE(LASERCA__Credit_Score_Experian__c),VALUE(LASERCA__Credit_Score_TransUnion__c)),VALUE(LASERCA__Credit_Score_Equifax__c)))
)</formula>
    <label>Credit Score (Median)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
