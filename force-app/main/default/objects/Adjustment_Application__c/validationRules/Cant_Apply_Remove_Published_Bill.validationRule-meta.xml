<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cant_Apply_Remove_Published_Bill</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISCHANGED(System_Bill__c), System_Bill__r.Account_Bill__r.Published__c = True)</errorConditionFormula>
    <errorMessage>Adjustment cannot be applied to or removed from a published bill</errorMessage>
</ValidationRule>
