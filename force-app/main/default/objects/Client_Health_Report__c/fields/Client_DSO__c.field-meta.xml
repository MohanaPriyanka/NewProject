<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Client_DSO__c</fullName>
    <externalId>false</externalId>
    <formula>IF(Clients_Invoiced__c &lt;= 0, 0, (Clients_Beginning_Balance__c + Clients_Invoiced__c - Clients_Payments__c)/(Clients_Invoiced__c/30))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Calculated from Customer Invoiced, Payments and Beginning Balance fields</inlineHelpText>
    <label>Client: DSO</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
